    <link name="ball">
      <inertial>
        <mass>0.0026</mass>
        <!-- inertia based on solid sphere 2/5 mr^2 -->
        <inertia>
          <ixx>1.66e-6</ixx>
          <iyy>1.66e-6</iyy>
          <izz>1.66e-6</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
